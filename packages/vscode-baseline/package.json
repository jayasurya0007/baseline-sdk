{
  "name": "baseline-compatibility",
  "displayName": "Baseline Compatibility",
  "publisher": "baseline-toolkit",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Linters"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:css"
  ],
  "main": "dist/extension.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayasurya0007/baseline-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/jayasurya0007/baseline-sdk/issues"
  },
  "homepage": "https://github.com/jayasurya0007/baseline-sdk#readme",
  "contributes": {
    "configuration": {
      "title": "Baseline",
      "properties": {
        "baseline.target": {
          "type": "string",
          "enum": [
            "limited",
            "newly",
            "widely"
          ],
          "default": "widely",
          "description": "Required Baseline target"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@types/vscode": "^1.92.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "web-features": "^2.49.0"
  }
}
